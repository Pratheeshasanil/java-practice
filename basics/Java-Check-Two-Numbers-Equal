class demo{
    public static void main(String args[]){

        int num1 = 20;
        int num2 = 70;

        System.out.println(num1 == num2);

        if(num1 == num2){
            System.out.println("Both are equal");

        }
        else{
            System.out.print("Both are not equal");
        }
    }
}
